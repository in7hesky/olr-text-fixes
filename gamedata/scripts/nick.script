		olr_timers.st_timer("del_rnd_zakaz_metka", 1500, "nick.del_rnd_zakaz_metka("..tostring(metka.id)..")")
	end
	if math.random(1,2) == 1 then
		alife():create("agr_stalker_regular", level.vertex_position(lv), lv, gv_max)
	end
        local text_phrase = {"У меня тут особый заказ есть. Тащи все, что найдешь странного: никогда не знаешь, какой сюрприз преподнесет в очередной раз Зона. За неизвестный артефакт десятикратный тариф, и поспеши, ты такой не один.", 
