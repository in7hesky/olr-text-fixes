			db.actor:give_talk_message(""..m_where.."", "ui\\ui_iconstotal", Frect():set(0,0,10,10), "simple_answer_item")
			return
		end
		local m_where = "Отлично, замер пошёл... Принимайте информацию об экземпляре."
		olr.info(known_inf_desc)
		news_manager.send_tip(db.actor, "Отданы деньги - 250.", nil, "money", 39000)
		db.actor:give_money(-250)
		game_stats.money_quest_update(-250)
		db.actor:give_talk_message(""..m_where.."", "ui\\ui_iconstotal", Frect():set(0,0,10,10), "simple_answer_item")
		return
	end
	local m_where = "Хм, какой-то сбой: что-то создаёт помехи на вашем поясе артефактов."
	db.actor:give_talk_message(""..m_where.."", "ui\\ui_iconstotal", Frect():set(0,0,10,10), "simple_answer_item")
