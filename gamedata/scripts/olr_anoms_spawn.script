		    db.actor:give_info_portion("spawn_anoms_dol")
                    spawn_dolina()
                 end

		if level_name == "lvl5_bar" and not has_alife_info("spawn_anoms_bar") then
		    db.actor:give_info_portion("spawn_anoms_bar")
