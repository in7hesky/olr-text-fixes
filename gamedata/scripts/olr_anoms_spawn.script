local obj = olr_anoms_spawn.spawn_anom(section, math.random(1,2), vl_pos,lv,gv_max, 0.1 ,bit_not(0))  -- math.random(60,90)
xvars.rnd_anoms_count = xvars.rnd_anoms_count + 1
