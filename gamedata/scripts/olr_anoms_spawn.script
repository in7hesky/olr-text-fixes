function spawn_esc()
local sectionsa = 
{ 
"zone_radioactive",

"zone_mosquito_bald_new_one",
"zone_mosquito_bald_new_two",
"zone_mosquito_bald_new_tri",
"zone_mosquito_bald_new_four",
